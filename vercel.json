{
    "version": 2,
    "functions": {
        "api/resettingDailyTasks.js": {
            "cron": "*/10 * * * *"
        }
    },
    "builds": [
        { "src": "server.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}
